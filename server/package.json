{
  "name": "honestfood",
  "version": "1.0.0",
  "description": "Honest food location",
  "main": "src/index.js",
  "author": "Malinda Ranasinghe",
  "private": true,
  "dependencies": {
    "bluebird": "^3.5.5",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "gtran-kml": "^1.2.3",
    "helmet": "^3.20.0",
    "morgan": "^1.9.1",
    "nodemon": "^1.19.1",
    "xml2json": "^0.11.2"
  },
  "devDependencies": {
    "babel": "^5.8.19",
    "chai": "^3.2.0",
    "eslint": "^3.15.0",
    "isparta": "^3.0.4",
    "istanbul": "^0.3.17",
    "mocha": "^2.2.5"
  },
  "scripts": {
    "dev": "NODE_ENV=development & nodemon src/index.js",
    "prod": "NODE_ENV=live & nodemon src/index.js",
    "pretest": "eslint src/",
    "test": "NODE_ENV=test mocha -R spec --timeout 10000 -c --harmony --require ./src/test/helper.js ./src/test/**/*.spec.js",
    "cover": "istanbul cover _mocha -- --ui bdd -R spec -t 10000 -r ./src/test/helper.js ./src/test/**/*.spec.js && npm run open:cover",
    "open:cover": "open coverage/lcov-report/index.html",
    "commit": "commit-wizard",
    "eslint": "eslint src/"
  },
  "release": {
    "analyzeCommits": "simple-commit-message"
  },
  "config": {
    "pre-git": {
      "commit-msg": "conventional",
      "pre-commit": "npm run test",
      "pre-push": [],
      "post-commit": "git status",
      "post-merge": "npm install"
    }
  }
}
